<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias alias="pd" type="com.sdhsie.base.util.PageData"/>
        <typeAlias type="com.sdhsie.web.system.model.Menu" alias="Menu"/>
        	<!-- 分页 -->
		<typeAlias type="com.sdhsie.base.util.Page" alias="Page"/>
    </typeAliases>
	<plugins>
		<plugin interceptor="com.sdhsie.base.plugin.PagePlugin">
			<property name="dialect" value="mysql"/>
			<property name="pageSqlId" value=".*Page.*"/>
		</plugin>
	</plugins>
    <mappers>
        <!-- 系统管理 -->
        <mapper resource="mybatis/system/user-mybatis.xml"/>
		<!-- 字典管理 -->
        <mapper resource="mybatis/system/dictionary-mybatis.xml"/>

        <!-- 角色管理 -->
         <mapper resource="mybatis/system/role-mybatis.xml"/>
        <!-- 菜单管理 -->
        <mapper resource="mybatis/system/menu-mybatis.xml"/>

        <!-- 组织管理 -->
        <mapper resource="mybatis/system/organize-mybatis.xml"/>

        <!-- 地区管理 -->
        <mapper resource="mybatis/system/area-mybatis.xml"/>

		<!-- 登录管理 -->
        <mapper resource="mybatis/system/login-mybatis.xml"/>
        <!-- 首页管理 -->
        <mapper resource="mybatis/system/main-mybatis.xml"/>

        <!-- 线路管理 -->
        <mapper resource="mybatis/system/road-mybatis.xml"/>

        <!-- 日志管理 -->
        <mapper resource="mybatis/system/log-mybatis.xml"/>
        
        <!-- 日志管理 -->
        <mapper resource="mybatis/system/quickMenu-mybatis.xml"/>
        <!-- apk版本管理 -->
        <mapper resource="mybatis/common/apkVersion-mybatis.xml"/>
        <!-- 用户反馈管理 -->
         <mapper resource="mybatis/common/feedback-mybatis.xml"/>
         <!--消息通知管理 -->
         <mapper resource="mybatis/common/message-mybatis.xml"/>
         <!--综合管理菜单管理 -->
         <mapper resource="mybatis/common/commonMenu-mybatis.xml"/>
         <!--综合管理角色管理 -->
         <mapper resource="mybatis/common/commonRole-mybatis.xml"/>
        <!-- 服务类型管理 -->
        <mapper resource="mybatis/serrver/category-mybatis.xml"/>
        <mapper resource="mybatis/serrver/categoryDetail-mybatis.xml"/>
        <mapper resource="mybatis/serrver/info-mybatis.xml"/>

        <!-- 客服人员管理 -->
        <mapper resource="mybatis/callCenter/telemarketer-mybatis.xml"/>
        <!-- 合作商信息管理 -->
        <mapper resource="mybatis/partners/partners-mybatis.xml"/>
        <!-- 合作商商户信息管理 -->
        <mapper resource="mybatis/partners/business-mybatis.xml"/>
        <!-- 设备用户信息维护 -->
        <mapper resource="mybatis/customer/customer-mybatis.xml"/>
        <!-- 设备初始化信息 -->
        <mapper resource="mybatis/customer/device-mybatis.xml"/>
        <!-- 家人信息 -->
        <mapper resource="mybatis/customer/family-mybatis.xml"/>
        <!-- 订单管理 -->
        <mapper resource="mybatis/order/order-mybatis.xml"/>
        <mapper resource="mybatis/order/callRecord_mybatis.xml"/>
        <mapper resource="mybatis/order/orderFlow-mybatis.xml"/>
        <mapper resource="mybatis/order/orderJob-mybatis.xml"/>
        <!--商户信息-->
        <mapper resource="mybatis/port/partner/user-mybatis.xml"/>
        <!--设备接口存储数据-->
        <mapper resource="mybatis/toro/userMessage-mybatis.xml"/>
        <!--回访管理-->
        <mapper resource="mybatis/visiting/visiting-mybatis.xml"/>
        <!--app展示信息维护-->
        <mapper resource="mybatis/appInfo/mallPhoto-mybatis.xml"/>
        <mapper resource="mybatis/appInfo/help-mybatis.xml"/>
        <!-- app商户登录 -->
         <mapper resource="mybatis/app/server/login/applogin-mybatis.xml"/>
        <!-- 商户日志 -->
         <mapper resource="mybatis/app/server/login/applog-mybatis.xml"/>
         <!-- 商户订单 -->
         <mapper resource="mybatis/app/server/order/apporder-mybatis.xml"/>
           <!-- 商户个人信息 -->
         <mapper resource="mybatis/app/server/personal/apppersonal-mybatis.xml"/>
           <!-- 商户我的 -->
         <mapper resource="mybatis/app/server/servermine/servermine-mybatis.xml"/>
            <!-- 家庭版首页 -->
         <mapper resource="mybatis/app/server/family/appindex-mybatis.xml"/>
          <!-- 家庭版服务 -->
         <mapper resource="mybatis/app/server/family/appserver-mybatis.xml"/>
           <!-- 家庭版我的 -->
         <mapper resource="mybatis/app/server/family/appmine-mybatis.xml"/>
           <!-- 家庭版订单 -->
         <mapper resource="mybatis/app/server/family/appfamilyorder-mybatis.xml"/>
          <!-- 消息 -->
         <mapper resource="mybatis/app/server/commons/appmessage-mybatis.xml"/>
        <!-- 手机端公共信息表 -->
        <mapper resource="mybatis/common/commonInfo-mybatis.xml"/>
    </mappers>
</configuration>